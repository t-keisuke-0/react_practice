{
    "env": {
        "browser": true, // windows, alert, document
        "node": true, // process, document
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module" //import/export
    },
    "rules": {
        "no-console": ["error"],
        "indent": ["error", 2],
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-unused-vars": ["warn"]
    }
}